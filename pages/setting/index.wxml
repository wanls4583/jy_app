<view class="container">
    <view class="white-background-color">
        <view class="padding-left-right-30 clickable">
            <view class="flex--center--between padding-top-bottom-30 border-bottom" bindtap="onOpenWebview" data-url="{{cretificationUrl}}">
                <text>资质证书</text>
                <view class="flex--center--center">
                    <van-icon name="arrow" color="#ccc" />
                </view>
            </view>
        </view>
        <view class="padding-left-right-30 clickable">
            <view class="flex--center--between padding-top-bottom-30 border-bottom" bindtap="onOpenWebview" data-url="{{serviceAgreementUrl}}">
                <text>服务条款</text>
                <view class="flex--center--center">
                    <van-icon name="arrow" color="#ccc" />
                </view>
            </view>
        </view>
        <view class="padding-left-right-30 clickable">
            <view class="flex--center--between padding-top-bottom-30 border-bottom" bindtap="onOpenWebview" data-url="{{privacyAgreementUrl}}">
                <text>隐私协议</text>
                <view class="flex--center--center">
                    <van-icon name="arrow" color="#ccc" />
                </view>
            </view>
        </view>
        <view class="padding-left-right-30 clickable">
            <view class="flex--center--between padding-top-bottom-30 border-bottom" bindtap="onOpenWebview" data-url="{{aboutUrl}}">
                <text>关于我们</text>
                <view class="flex--center--center">
                    <van-icon name="arrow" color="#ccc" />
                </view>
            </view>
        </view>
    </view>
</view>