.van-button--primary {
    background-color: rgb(42, 175, 255) !important;
    border-color: rgb(42, 175, 255) !important;
}

.van-button--plain {
    background-color: transparent !important;
}

.van-button--primary .van-button__text {
    color: #fff !important;
}

.van-button--primary.van-button--plain .van-button__text {
    color: rgb(42, 175, 255) !important;
}

.van-button--block {
    width: 100% !important;
}

.success-color .van-button__text,
.success-color .van-button {
    color: #07c160;
    border-color: #07c160;
}

.success-background-color .van-button {
    background-color: rgb(42, 175, 255) !important;
    border-color: rgb(42, 175, 255) !important;
}

.primary-color .van-button__text,
.primary-color .van-button,
.primary-color .van-icon {
    color: rgb(42, 175, 255);
    border-color: rgb(42, 175, 255);
}

page .van-circle__text {
    color: #fff;
    font-size: 20rpx;
}

page .van-tabs__line {
    background-color: rgb(42, 175, 255);
}

.dialog-index--van-dialog__confirm .van-button__text {
    color: rgb(42, 175, 255)
}

page .van-cell {
    padding: 30rpx;
}

page .van-cell:after {
    left: 30rpx;
    right: 30rpx;
}

page .van-cell__title {
    display: flex;
    align-items: center;
}

page .van-collapse-item__content {
    padding: 0
}

.van-collapse-item .item-index--van-cell {
    font-weight: bold;
}